<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" spacing="3.0">
         <children>
            <Label fx:id="title" text="Function F=F(x)" textFill="WHITE" />
            <HBox fx:id="hbox" alignment="CENTER" StackPane.alignment="CENTER_LEFT">
               <children>
                  <ComboBox fx:id="selection" cache="true" cacheHint="SPEED" editable="true" focusTraversable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <Label fx:id="res1" text="p1: val or error msg" HBox.hgrow="SOMETIMES" />
                  <Pane HBox.hgrow="ALWAYS" />
                  <Label fx:id="res2" layoutX="10.0" layoutY="10.0" text="p2: val" HBox.hgrow="SOMETIMES" />
               </children>
            </HBox>
            <VBox fx:id="subControls" alignment="CENTER" maxHeight="1.7976931348623157E308">
               <padding>
                  <Insets left="3.0" right="3.0" />
               </padding></VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
